{
    "name": "suilven/silverstripe-notifier",
    "description": "Notify to logs and optionally to other sources via configuration",
    "type": "silverstripe-vendormodule",
    "license": "MIT",
    "authors": [
        {
            "name": "Gordon Anderson",
            "email": "gordon.b.anderson@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Suilven\\Notifier\\": "src/",
            "Suilven\\Notifier\\Tests\\": "src/Tests"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "alek13/slack": "^1",
        "symbiote/silverstripe-queuedjobs": "^4.1"
    },
    "extra": {
        "installername": "slack-notifier"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "php-vcr/php-vcr": "^1.4@dev"
    }
}
